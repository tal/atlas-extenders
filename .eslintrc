env:
  node: true

rules:
  no-floating-decimal: 2
  no-nested-ternary: 2
  no-undefined: 2
  no-comma-dangle: 0
  no-unused-expressions: 0
  no-underscore-dangle: 0
  no-multi-spaces: 0
  camelcase: 1
  brace-style:
    - 2
    - "1tbs"
  comma-style:
    - 2
    - "last"
  default-case: 2
  func-style:
    - 2
    - "declaration"
  guard-for-in: 2
  radix: 2
  space-after-function-name:
    - 2
    - "never"
  space-after-keywords:
    - 2
    - "always"
  space-before-blocks: 2
  spaced-line-comment:
    - 2
    - "always"
    - { exceptions: ["-"]}
  strict:
    - 0
    - "global"
  valid-jsdoc:
    - 2
    - { prefer: { "return": "returns"}}
  wrap-iife: 2
  quotes:
    - 1
    - "single"
    - "avoid-escape"
